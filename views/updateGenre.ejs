<!DOCTYPE html>
<html>
  <head>
    <title><%=title%></title>
  </head>
  <body>
    <h1><%=title%></h1>
    <a href="/">All Movies</a>
    <a href="/addMovie">Add Movie</a>
    <a href="/genres">Genres</a>
    <form action="/genres/<%=genre.genre_id%>/update" method="POST">
      <label for="genre_name">Genre:</label>
      <input
        type="text"
        name="genre_name"
        id="genre_name"
        value="<%=genre.genre_name%>"
      />
      <button type="submit">Add</button>
    </form>
  </body>
</html>
